{
	"scripts": {
		"postinstall": "run-p fix:types fix:gun:spam",
		"fix:types": "rm -rf node_modules/vuex/types/vue.d.ts",
		"fix:gun:spam": "open node_modules/gun/gun.js",
		"clean": "rm -rf dist && rm -rf node_modules",
		"client:dev": "cross-env NODE_ENV=development vue-cli-service serve",
		"client:dev-legacy": "rm -rf dist/client && cross-env NODE_ENV=development webpack --config node_modules/@vue/cli-service/webpack.config.js",
		"client:build": "cross-env NODE_ENV=production vue-cli-service build",
		"server:dev": "rm -rf dist/server && tsc-watch -p src/server/server.tsconfig.json --onSuccess 'npm run server:forever:dev'",
		"server:forever:dev": "cross-env NODE_ENV=development node dist/server/main.js",
		"server:forever:dev:legacy": "cross-env NODE_ENV=development forever --killTree --minUptime 1000 --spinSleepTime 10000 dist/server/main.js"
	},
