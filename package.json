{
	"name": "robinhood.tools",
	"description": "Advanced Trading Tools & Resources for Robinhood Web/Crypto",
	"version": "0.1.0",
	"domain": "robinhood.tools",
	"scripts": {
		"clean": "rimraf dist node_modules",
		"client:dev": "cross-env NODE_ENV=development vue-cli-service serve",
		"client:build": "cross-env NODE_ENV=production vue-cli-service build",
		"server:dev": "rimraf dist/server && tsc-watch -p config/server.tsconfig.json --onSuccess 'npm run server:start'",
		"server:start": "cross-env NODE_ENV=development node dist/server/main.js",
		"server:build": "rimraf dist/server && tsc -p config/server.tsconfig.json"
	},
