{
	"name": "robinhood.tools",
	"description": "Advanced trading tools and resources for Robinhood Web.",
	"version": "0.0.1",
	"domain": "robinhood.tools",
	"scripts": {
		"postinstall": "run-p patch:*",
		"patch:types": "rimraf node_modules/tiny-emitter/index.d.ts node_modules/vuex/types/vue.d.ts",
		"patch:uws": "rimraf node_modules/uws/uws.js && cp -f src/server/modules/uws.js node_modules/uws/uws.js",
		"purge": "rimraf dist node_modules",
		"client:dev": "cross-env NODE_ENV=development vue-cli-service serve --mode development",
		"client:build": "rimraf dist/client && cross-env NODE_ENV=production vue-cli-service build --mode production",
		"rs": "pandora dev",
		"server:dev:pm2": "pm2 list && rimraf dist/server && tsc-watch -p .server.tsconfig.json --onSuccess 'pm2-runtime pm2.config.json --raw'",
		"server:dev:forever": "rimraf dist/server && tsc-watch -p .server.tsconfig.json --onSuccess 'forever dist/server/main.js'",
		"server:dev:pandora": "pandora exit && pandora start && pandora log robinhood.tools",
		"server:build": "rimraf dist/server && tsc -p .server.tsconfig.json",
		"pandora": "run-p pandora:*",
		"pandora:procfile": "tsc src/procfile.ts -outDir . --watch",
		"pandora:watch": "tsc -p .server.tsconfig.json --watch",
		"cli:clear": "clear && printf \"\\e[3J\""
	},
