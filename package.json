{
	"name": "robinhood.tools",
	"description": "Advanced trading tools and resources for Robinhood Web.",
	"version": "0.0.1",
	"domain": "robinhood.tools",
	"scripts": {
		"postinstall": "run-p patch:types client:tailwind",
		"patch:types": "rimraf node_modules/vuex/types/vue.d.ts",
		"purge": "rimraf dist node_modules",
		"rc": "run-s client:tailwind client:dev",
		"client:dev": "cross-env NODE_ENV=development vue-cli-service serve --mode development",
		"client:build": "rimraf dist/client && cross-env NODE_ENV=production vue-cli-service build --mode production",
		"client:tailwind": "tailwind build node_modules/tailwindcss/utilities.css -c src/client/styles/tailwind.js -o src/client/styles/tailwind.css",
		"rs": "npm run server:dev:watch",
		"server:dev:watch": "rimraf dist/server && tsc-watch -p .server.tsconfig.json --onSuccess 'forever forever.config.json'",
		"server:dev:forever": "forever forever.config.json --minUptime=1000 --spinSleepTime=1000",
		"server:dev:node": "rimraf dist/server && tsc-watch -p .server.tsconfig.json --onSuccess 'node dist/server/main.js'",
		"server:dev:pm2": "rimraf dist/server && pm2 list && tsc-watch -p .server.tsconfig.json --onSuccess 'pm2-runtime pm2.config.json'",
		"server:build": "rimraf dist/server && tsc -p .server.tsconfig.json",
		"cli:clear": "clear && printf \"\\e[3J\""
	},
