{
	"apps": [{
		"name": "robinhood.tools",
		"script": "dist/server/main.js",
		// "exec_interpreter" : "ts-node",
		"args": ["--color"],
		"node_args": ["--max_old_space_size=512"],
		// 
		"exec_mode": "fork",
		"instances": "max",
		// 
		"automation": false,
		"pmx": false,
		"vizion": false,
		// 
		"wait_ready": false,
		"kill_timeout": 10000,
		// 
		"watch": false,
		"autorestart": false,
		"max_memory_restart": "512M",
		"max_restarts": 3,
		"min_uptime": "3s",
		"restart_delay": 3000,
		"merge_logs": true,
		"out_file": "/dev/null",
		"error_file": "/dev/null",
		"source_map_support": true,
		// 
		"env": {
			"NODE_ENV": "development"
		},
		"env_production": {
			"NODE_ENV": "production"
		}
		// 
	}]
}
